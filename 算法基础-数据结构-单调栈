#include <iostream>
#include <stack>

using namespace std;

const int N = 100000 + 10;
int a[N];
int n;


int main()
{
	stack<int> stk;

	cin >> n;

	for (int i = 0; i < n; i++)
	{
		cin >> a[i];
		while (stk.size() != 0 && stk.top() > a[i])
		{
			stk.pop();
		}

		if (!stk.empty()) cout << stk.top()<<" ";
		else
		{
			cout << -1 << " ";
		}

		stk.push(a[i]);
	}
}
